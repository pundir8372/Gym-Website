{
  "version": 2,
  "projects": [
    {
      "name": "gym-website-frontend",
      "rootDirectory": "frontend",
      "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "dist"
          }
        }
      ],
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
